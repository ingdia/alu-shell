#!/usr/bin/env bash
# Script to forcefully kill the 7-highlander process.

# Find the PID(s) of 7-highlander
pro_pid=$(pgrep -x "7-highlander")

# Ensure the process is running before attempting to kill it
if [[ -n "$pro_pid" ]]; then
    kill -9 "$pro_pid"  # Forcefully kill the process
    echo "OK"           # Expected output for validation
else
    echo "Process 7-highlander is not running."
    exit 1
fi

